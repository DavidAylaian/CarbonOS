OBJS=		\
strcat.o	\
strcmp.o	\
strcpy.o	\
strlen.o	\
strrev.o	\
itoa.o		\

build:
	$(CC) $(CFLAGS) strcat.c	-o strcat.o
	$(CC) $(CFLAGS) strcmp.c	-o strcmp.o
	$(CC) $(CFLAGS) strcpy.c	-o strcpy.o
	$(CC) $(CFLAGS) strlen.c	-o strlen.o
	$(CC) $(CFLAGS) strrev.c	-o strrev.o
	$(CC) $(CFLAGS) itoa.c		-o itoa.o

clean:
	rm -f $(OBJS)
